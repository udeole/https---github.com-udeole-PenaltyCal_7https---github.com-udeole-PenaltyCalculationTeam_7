--Create party, security_penalty_rate,login_user,security_price first and add data to it.
CREATE TABLE transaction(
transaction_id int not null Primary Key GENERATED ALWAYS AS IDENTITY,
   place_of_holding_tech_number  smallint not null,
   isin  char(12) not null,
   security_quantity  int not null,
	transaction_type_code  char(4) not null,
   instruction_type_code   char(4) not null,
   matching_reference  char(16) not null,
	settlement_date Date not null,
	settlement_cash_amount Decimal(6,2) not null,
	calendar_id char(10) not null,
	party_id char(16) not null,
	penalty_id bigint not null,
	price_id bigint not null,
	login_id int not null,
	counter_party_id char(16) not null,
	party_role_cd char(6) not null,
	counter_party_role_cd char(6) not null,
	failing_party_role_cd char(6) not null,
	penalty_amount Decimal(6,3) not null,
	sign Bit not null,
	cntl_timestamp Timestamp not null,
	cntl_userid char(20) not null,
	FOREIGN KEY (party_id) REFERENCES Party(party_id),
	FOREIGN KEY (Penalty_Id) REFERENCES security_penalty_rate(Penalty_id),
	FOREIGN KEY (login_id) REFERENCES login_user(login_id),
	FOREIGN KEY (price_id) REFERENCES security_price(price_id)
	
	
);



INSERT INTO Transaction (
   Place_of_Holding_Tech_number,
   ISIN,
   Security_quantity,
   Transaction_Type_Code,
   Instruction_Type_Code,
   Matching_Reference,
   Settlement_Date,
